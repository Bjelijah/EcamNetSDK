<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.howell.ecamnetsdk.com.demo.ecamnetsdk.MainActivity">




    <Button
        android:id="@+id/btn_login_server"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登入系统"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />


    <Button
        android:id="@+id/btn_module_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="视频模块"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_login_server"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/btn_login_server" />

    <Button
        android:id="@+id/btn_module_vehicle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="车辆模块"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_module_video"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/btn_module_video" />

    <Button
        android:id="@+id/btn_logout_server"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登出系统"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_module_vehicle"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/btn_module_vehicle" />


</android.support.constraint.ConstraintLayout>
